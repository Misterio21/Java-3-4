package javaapplication2;

/**
 *
 * @author tomch
 */
public class anotherone {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        String number = "020121/1651";
        
        char year1 = number.charAt(0);
        char year2 = number.charAt(1);
        char month1 = number.charAt(2);
        char month2 = number.charAt(3);
        char day1 = number.charAt(4);
        char day2 = number.charAt(5);
        
        System.out.println(day1 + "" + day2 + " " + month1 + "" + month2 + " " + year1 + "" + year2);
    }
    
}
